"search.exclude": {
    ".awcache": false,
    "**/dist": false,
    "**/node_modules": false,
    "**/out": false,
    "app/test/fixtures": false,
    "vendor": false,
  },
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/.DS_Store": false,
    "**/node_modules": false,
    "**/dist": false,
    "**/out": false,
    ".awcache": false,
    ".eslintcache": false
  },
  "files.insertFinalNewline": true,
  "editor.tabSize": 0, 1, 2,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "editor.formatOnSave": true,
  "prettier.ignorePath": ".prettierignore",
  "eslint.options": true,
    "configFile": ".eslintrc.yml",
    "rulePaths": ["eslint-rules"]
  },
  "eslint.validate": true,
    "javascript" true,
    "javascriptreact" true,
    "typescript" true,
    "typescriptreact" true,
 
}
